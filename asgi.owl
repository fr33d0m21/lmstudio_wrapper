<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/asgi-onto.owl#"
     xml:base="http://example.org/asgi-onto.owl"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://example.org/asgi-onto.owl"/>
    
    <!-- Classes -->
    <owl:Class rdf:about="http://example.org/asgi-onto.owl#ArtificialIntelligence"/>
    
    <owl:Class rdf:about="http://example.org/asgi-onto.owl#ASGI">
        <rdfs:subClassOf rdf:resource="http://example.org/asgi-onto.owl#ArtificialIntelligence"/>
        <rdfs:comment>Artificial Super General Intelligence</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/asgi-onto.owl#CognitiveCapability">
        <rdfs:subClassOf rdf:resource="http://example.org/asgi-onto.owl#ArtificialIntelligence"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/asgi-onto.owl#LearningMechanism">
        <rdfs:subClassOf rdf:resource="http://example.org/asgi-onto.owl#ArtificialIntelligence"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/asgi-onto.owl#KnowledgeRepresentation">
        <rdfs:subClassOf rdf:resource="http://example.org/asgi-onto.owl#ArtificialIntelligence"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/asgi-onto.owl#ReasoningEngine">
        <rdfs:subClassOf rdf:resource="http://example.org/asgi-onto.owl#ArtificialIntelligence"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/asgi-onto.owl#EthicalFramework">
        <rdfs:subClassOf rdf:resource="http://example.org/asgi-onto.owl#ArtificialIntelligence"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/asgi-onto.owl#ConsciousnessModel">
        <rdfs:subClassOf rdf:resource="http://example.org/asgi-onto.owl#ArtificialIntelligence"/>
    </owl:Class>
    
    <owl:Class rdf:about="http://example.org/asgi-onto.owl#GoalAlignment">
        <rdfs:subClassOf rdf:resource="http://example.org/asgi-onto.owl#ArtificialIntelligence"/>
    </owl:Class>
    
    <!-- Object Properties -->
    <owl:ObjectProperty rdf:about="http://example.org/asgi-onto.owl#Possesses">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#ASGI"/>
        <rdfs:range rdf:resource="http://example.org/asgi-onto.owl#CognitiveCapability"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/asgi-onto.owl#Utilizes">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#ASGI"/>
        <rdfs:range rdf:resource="http://example.org/asgi-onto.owl#LearningMechanism"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/asgi-onto.owl#Implements">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#ASGI"/>
        <rdfs:range rdf:resource="http://example.org/asgi-onto.owl#KnowledgeRepresentation"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/asgi-onto.owl#OperatesOn">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#ASGI"/>
        <rdfs:range rdf:resource="http://example.org/asgi-onto.owl#ReasoningEngine"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/asgi-onto.owl#AdheresTo">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#ASGI"/>
        <rdfs:range rdf:resource="http://example.org/asgi-onto.owl#EthicalFramework"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/asgi-onto.owl#Exhibits">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#ASGI"/>
        <rdfs:range rdf:resource="http://example.org/asgi-onto.owl#ConsciousnessModel"/>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://example.org/asgi-onto.owl#AimsFor">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#ASGI"/>
        <rdfs:range rdf:resource="http://example.org/asgi-onto.owl#GoalAlignment"/>
    </owl:ObjectProperty>
    
    <!-- Data Properties -->
    <owl:DatatypeProperty rdf:about="http://example.org/asgi-onto.owl#IntelligenceQuotient">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#ASGI"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/asgi-onto.owl#LearningRate">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#LearningMechanism"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <owl:DatatypeProperty rdf:about="http://example.org/asgi-onto.owl#EthicalComplianceScore">
        <rdfs:domain rdf:resource="http://example.org/asgi-onto.owl#EthicalFramework"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    
    <!-- Individuals -->
    <owl:NamedIndividual rdf:about="http://example.org/asgi-onto.owl#SuperAI">
        <rdf:type rdf:resource="http://example.org/asgi-onto.owl#ASGI"/>
        <Possesses rdf:resource="http://example.org/asgi-onto.owl#GeneralProblemSolving"/>
        <Utilizes rdf:resource="http://example.org/asgi-onto.owl#DeepReinforcementLearning"/>
        <Implements rdf:resource="http://example.org/asgi-onto.owl#SemanticNetwork"/>
        <OperatesOn rdf:resource="http://example.org/asgi-onto.owl#BayesianInference"/>
        <AdheresTo rdf:resource="http://example.org/asgi-onto.owl#UtilitarianEthics"/>
        <Exhibits rdf:resource="http://example.org/asgi-onto.owl#IntegratedInformationTheory"/>
        <AimsFor rdf:resource="http://example.org/asgi-onto.owl#HumanCompatibleGoals"/>
        <IntelligenceQuotient>1000.0</IntelligenceQuotient>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://example.org/asgi-onto.owl#GeneralProblemSolving">
        <rdf:type rdf:resource="http://example.org/asgi-onto.owl#CognitiveCapability"/>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://example.org/asgi-onto.owl#DeepReinforcementLearning">
        <rdf:type rdf:resource="http://example.org/asgi-onto.owl#LearningMechanism"/>
        <LearningRate>0.01</LearningRate>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://example.org/asgi-onto.owl#SemanticNetwork">
        <rdf:type rdf:resource="http://example.org/asgi-onto.owl#KnowledgeRepresentation"/>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://example.org/asgi-onto.owl#BayesianInference">
        <rdf:type rdf:resource="http://example.org/asgi-onto.owl#ReasoningEngine"/>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://example.org/asgi-onto.owl#UtilitarianEthics">
        <rdf:type rdf:resource="http://example.org/asgi-onto.owl#EthicalFramework"/>
        <EthicalComplianceScore>0.95</EthicalComplianceScore>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://example.org/asgi-onto.owl#IntegratedInformationTheory">
        <rdf:type rdf:resource="http://example.org/asgi-onto.owl#ConsciousnessModel"/>
    </owl:NamedIndividual>
    
    <owl:NamedIndividual rdf:about="http://example.org/asgi-onto.owl#HumanCompatibleGoals">
        <rdf:type rdf:resource="http://example.org/asgi-onto.owl#GoalAlignment"/>
    </owl:NamedIndividual>
</rdf:RDF>
